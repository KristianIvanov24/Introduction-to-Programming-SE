#include <cstring>
#include <iostream>

const char WILDCARD = '.';

bool isDigit(char c)
{
    return c >= '0' && c <= '9';
}

bool isLetter(char c)
{
    return c >= 'a' && c <= 'z';
}

int digitToInt(char c)
{
    return c - '0';
}

char* substring(const char* source, size_t start, size_t len)
{
    if (!source) {
        return new char[1] {};
    }

    char* result = new char[len + 1]{};
    for (size_t i = 0; i < len; i++)
    {
        result[i] = source[start + i];
    }

    return result;
}

bool isValidSequenceChar(const char* str, char targetLetter)
{
    return *str == targetLetter || *str == WILDCARD;
}

int prefixSequenceLen(const char* str)
{
    if (!str || !isDigit(*str))
    {
        return 0;
    }

    int requiredCount = digitToInt(*str);
    if (requiredCount == 0)
    {
        return 0;
    }
    str++;

    char targetLetter = *str;
    if (!isLetter(targetLetter))
    {
        return 0;
    }
    str++;

    int matchedCount = 1;
    while (*str && matchedCount < requiredCount)
    {
        if (!isValidSequenceChar(str, targetLetter))
        {
            break;
        }

        matchedCount++;
        str++;
    }

    if (matchedCount == requiredCount)
    {
        return matchedCount + 1;
    }

    return 0;
}

char* extractLongestSequence(const char* str)
{
    if (!str)
    {
        return new char[1]{};
    }

    int maxLen = 0;
    int maxStart = -1;
    int len = strlen(str);

    for (int i = 0; i < len; i++)
    {
        int seqLen = prefixSequenceLen(str + i);

        if (seqLen > maxLen)
        {
            maxLen = seqLen;
            maxStart = i;
        }
    }

    if (maxStart == -1)
    {
        return new char[1]{};
    }

    return substring(str, maxStart, maxLen);
}

int main()
{
    const int MAX_LEN = 1000;
    char str[MAX_LEN];

    std::cin >> str;

    char* result = extractLongestSequence(str);
    std::cout << result << std::endl;

    delete[] result;

    return 0;
}
